{
  "manifest_version": 3,
  "name": "YouTube Timestamp Clipper",
  "version": "1.0",
  "description": "YouTutbeの再生時間とタイトルを保存します",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?v=*"],
      "js": ["content.js"]
    }
  ]
}