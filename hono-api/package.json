{
  "name": "hono-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply hono-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply hono-db --remote"
  },
  "dependencies": {
    "@google/genai": "^1.43.0",
    "better-auth": "^1.4.18",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260214.0",
    "drizzle-kit": "^0.31.9",
    "typescript": "^5.7.4",
    "wrangler": "^4.65.0"
  }
}